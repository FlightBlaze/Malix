object obj {
    fn: def (one, two) {
        return one + two
    },
    one: 1,
    obj: object {
        two: 2
    }
}

println obj
println callFunction(obj->fn, [obj->one, obj->obj->two]) ;; 3


object nums {
    numbers: object {
        one: nil,
        two: nil
    },
    function: def (one, two) {
        return one + two
    },
    array: [
        1, 2
    ]
}

var arrayFromObject = nums->array

nums->numbers->one = arrayFromObject[0]
nums->numbers->two = arrayFromObject[1]

println nums
println callFunction(nums->function, [nums->numbers->one, nums->numbers->two]) ;; 3